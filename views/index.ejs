        <div id="login-container">
          <form name="login form" style="
          width: auto;
          height: 50vh;
          display: block;
          position: relative;
          margin: 1vh;
          padding: 2vh;">
            <h1 class="oswald text-center" style="font-weight: 600; color: #666666; padding-bottom:2vh">LOGIN</h1>

            <div class="mb-3" style="padding-bottom: 1vh;">
              <label for="username" class="form-label montserrat login-label" style="font-size: 20px;">Username</label>
              <input type="text" class="form-control text-center" style="background: #FEF9F9;
              border: 1px solid #C4C4C4;
              box-sizing: border-box;
              box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);
              border-radius: 10px;">
            </div>

            <div class="mb-3" style="padding-bottom: 3vh;">
              <label for="password" class="form-label montserrat login-label" style="font-size: 20px;">Password</label>
              <input type="password" class="form-control text-center" style="background: #FEF9F9;
              border: 1px solid #C4C4C4;
              box-sizing: border-box;
              box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);
              border-radius: 10px;">
            </div>

            <div class="mb-3 text-center"> 
              <button type="submit" class="btn btn-primary" style="
              width:7vw;
              height:auto;
              font-size: 20px;
              background: #6CD9B1;
              border: #6CD9B1;
              color:#FEF9F9;
              box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);
              border-radius: 10px;">Login</button>
            </div>
            <p class="text-center montserrat" style="font-size:small">Don't have an account yet? 
              <a class="register-link" href="/register" style="color: #F277A4;">Register</a>
            </p>
          </form>
